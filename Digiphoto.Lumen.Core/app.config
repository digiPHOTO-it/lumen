<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Digiphoto.Lumen.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="Digiphoto.Lumen.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>

    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />

  </configSections>

  <log4net>
    <appender name="FileAppender" type="log4net.Appender.FileAppender">
      <file value="c:\temp\log4net.log" />
      <appendToFile value="false" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %thread %-5level %logger{1} - %message%newline" />
      </layout>
    </appender>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %thread %-5level %logger{1} - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="FileAppender" />
      <appender-ref ref="ConsoleAppender" />
    </root>
  </log4net>


  <userSettings>
    <Digiphoto.Lumen.Properties.Settings>
      <setting name="giorniDeleteFoto" serializeAs="String">
        <value>8</value>
      </setting>
      <setting name="dbCartella" serializeAs="String">
        <value />
      </setting>
      <setting name="cartellaFoto" serializeAs="String">
        <value />
      </setting>
      <setting name="dbCartella2" serializeAs="String">
        <value>%PROGRAMDATA%\Lumen</value>
      </setting>
      <setting name="dbCartella1" serializeAs="String">
        <value>%APPDATA%\Lumen</value>
      </setting>
      <setting name="pixelLatoProvino" serializeAs="String">
        <value>300</value>
      </setting>
    </Digiphoto.Lumen.Properties.Settings>
  </userSettings>
  <applicationSettings>
    <Digiphoto.Lumen.Properties.Settings>
      <setting name="dbNomeDbVuoto" serializeAs="String">
        <value>dbvuoto.sdf</value>
      </setting>
      <setting name="dbNomeDbPieno" serializeAs="String">
        <value>database.sdf</value>
      </setting>
      <setting name="oraCambioGiornata" serializeAs="String">
        <value>05:30</value>
      </setting>
      <setting name="estensioniGrafiche" serializeAs="String">
        <value>*.jpg;*.jpeg;*.png;*.tif;*.tiff</value>
      </setting>
    </Digiphoto.Lumen.Properties.Settings>
  </applicationSettings>
  
</configuration>