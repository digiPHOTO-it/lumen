<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/Resources/GlobalResourceDictionary.xaml" />
	</ResourceDictionary.MergedDictionaries>

	<DataTemplate x:Key="fotografiListBoxTemplate">

        <Border BorderBrush="Red" >
            <StackPanel Orientation="Horizontal">
                <Label Content="{Binding iniziali}" />
                <Label Content="{Binding cognomeNome}" />
            </StackPanel>
        </Border>

    </DataTemplate>

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

    <Style x:Key="newNomeFotografoTxtStyle" TargetType="{x:Type TextBox}">
        <Setter Property="CharacterCasing" Value="Upper" />
    </Style>
    
    <Style x:Key="newInizialiFotografoTxtStyle" TargetType="{x:Type TextBox}">
        <Setter Property="CharacterCasing" Value="Upper" />
    </Style>

	
	
	
	
</ResourceDictionary>